<resources>

    <style name="Widget.Material3.Button.SearchBar" parent="Widget.Material3.Button.ElevatedButton.Icon">
        <item name="android:backgroundTint">?attr/colorPrimaryContainer</item>
        <item name="cornerRadius">@dimen/search_bar_radius</item>
        <item name="android:paddingHorizontal">@dimen/search_bar_padding_horizontal</item>
        <item name="android:paddingVertical">@dimen/search_bar_padding_vertical</item>
        <item name="android:textAppearance">@style/TextAppearance.Material3.LabelLarge</item>
    </style>

    <style name="WallpaperRoundedImageView">
        <item name="cornerFamily">rounded</item>
        <item name="cornerSize">@dimen/wallpaper_image_radius</item>
    </style>

    <style name="HomeProfileImageView">
        <item name="cornerFamily">rounded</item>
        <item name="cornerSize">@dimen/home_profile_radius</item>
    </style>

    <style name="AboutProfileImageView">
        <item name="cornerFamily">rounded</item>
        <item name="cornerSize">@dimen/about_profile_radius</item>
    </style>
</resources>